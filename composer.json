{
  "name":        "ebidtech/storage-client",
  "description": "Multi provider storage service",
  "license":     "MIT",
  "homepage":    "https://github.com/ebidtech/storage-client",
  "type":        "library",
  "keywords":    [],
  "require": {
    "php":                       ">=5.5.0",
    "psr/log":                   "1.0.0",
    "ebidtech/options-resolver": "1.0.*",
    "ebidtech/compress":         "1.1.*",
    "aws/aws-sdk-php":           "3.2.*"
  },
  "require-dev": {
    "phpunit/phpunit":           "4.7.*",
    "nikic/php-parser":          "1.3.*",
    "squizlabs/php_codesniffer": "2.3.*"
  },
  "scripts": {
    "post-install-cmd": [
      "bash ./tools/install-codesniffer.sh"
    ]
  },
  "autoload": {
    "psr-0": {
      "EBT\\StorageClient": "src/"
    }
  },
  "minimum-stability": "stable"
}
